{
  "name": "aws-sigv4",
  "version": "1.2.0",
  "description": "AWS Signature Version 4",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "gulp checkSuite",
    "test": "mocha test/**/*.es6 --compilers js:babel-core/register",
    "coverage": "`npm bin`/babel-node `npm bin`/istanbul cover _mocha -- -R spec --ui tdd"
  },
  "keywords": [
    "aws",
    "signature"
  ],
  "author": "Barrett K. Harber <barrett.harber@gmail.com>",
  "license": "Unlicense",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "bluebird": "^3.3.1",
    "coveralls": "^2.11.4",
    "eslint": "^2.0.0",
    "gulp": "^3.9.0",
    "gulp-download": "0.0.1",
    "gulp-unzip": "^0.1.3",
    "gulp-util": "^3.0.7",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-eslint": "^1.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "rules": {
      "indent": [
        2,
        "tab"
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ]
    },
    "env": {
      "es6": true,
      "browser": false
    },
    "extends": "eslint:recommended"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binoculars/aws-sigv4.git"
  },
  "bugs": {
    "url": "https://github.com/binoculars/aws-sigv4/issues"
  },
  "homepage": "https://binoculars.github.io/aws-sigv4/"
}
