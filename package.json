{
  "name": "aws-sigv4",
  "version": "1.6.0",
  "description": "AWS Signature Version 4",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "gulp checkSuite",
    "test": "nyc mocha test/**/*.es6 --compilers js:babel-core/register,js:babel-polyfill",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "aws",
    "signature"
  ],
  "author": "Barrett K. Harber <barrett.harber@gmail.com>",
  "license": "Unlicense",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "coveralls": "^2.11.4",
    "eslint": "^2.1.0",
    "gulp": "^3.9.0",
    "gulp-download": "0.0.1",
    "gulp-unzip": "^0.1.3",
    "gulp-util": "^3.0.7",
    "mocha": "^2.3.3",
    "mocha-eslint": "^2.0.2",
    "nyc": "^6.4.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "nyc": {
    "extension": [
      ".es6"
    ],
    "exclude": [
      "**/*.js"
    ],
    "include": [
      "**/*.es6"
    ],
    "report-dir": "./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binoculars/aws-sigv4.git"
  },
  "bugs": {
    "url": "https://github.com/binoculars/aws-sigv4/issues"
  },
  "homepage": "https://binoculars.github.io/aws-sigv4/"
